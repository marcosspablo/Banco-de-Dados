CREATE TABLE PRODUTOS ( 
CODIGO INT NOT NULL,
NOME VARCHAR(50),
DESCRICAO VARCHAR(255),
TIPO VARCHAR(30),
GENERO VARCHAR(20)
);


CREATE TABLE PLANOS (
CODIGO INT NOT NULL,
NOME VARCHAR(55),
DESCRICAO VARCHAR(255),
TIPO VARCHAR(50),
VALOR VARCHAR(10)
);


CREATE TABLE CLIENTE (
CODIGO AUTO INCREMENTO,
NOME VARCHAR(50),
ENDERECO VARCHAR(255),
EMAIL VARCHAR(50),
TELEFONE VARCHAR(50),
COD_PLANO VARCHAR(50),
SENHA VARCHAR(20)
);
GO

CREATE TABLE ALUGUEL(
CODIGO INT NOT NULL,
COD_PRODUTO VARCHAR(55) NOT NULL,
COD_FORMA_DE_PAGAMENTO VARCHAR (40) NOT NULL,
COD_CLIENTE VARCHAR(15) NOT NULL 
);


CREATE TABLE FORMA_PAGAMENTO(
CODIGO INT NOT NULL,
DESCRICAO VARCHAR(50)
);


CREATE TABLE TELEFONE(
COGIDO INT NOT NULL,
TELEFONE_CLIENTE VARCHAR(13)  NOT NULL,
OPERADORA VARCHAR(10)  NOT NULL,
DDD VARCHAR(10) NOT NULL
);


CREATE TABLE ENDERECO (
CODIGO INT NOT NULL,
LOGRADOURO VARCHAR(35),
NUMERO VARCHAR(10),
BAIRRO VARCHAR(15),
CEP VARCHAR(10),
COMPLEMENTO VARCHAR(15),
CLIENTE_ID VARCHAR(30)
);
